<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">

<!-- Welcome-file-list called parent file -->
<welcome-file-list>
   <welcome-file>Home.html</welcome-file>
</welcome-file-list>


<!-- servelet declaration -->

<servlet>
<servlet-name>register</servlet-name>
<servlet-class>com.servelets.RegisterServletDemo</servlet-class>
</servlet>

<servlet>
<servlet-name>success</servlet-name>
<servlet-class>com.servelets.SuccessServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>register</servlet-name>
<url-pattern>/RegisterServletDemo</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>success</servlet-name>
<url-pattern>/success</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>Sum</servlet-name>
<servlet-class>com.attr.S1</servlet-class>
</servlet>

<servlet>
<servlet-name>Ssum1</servlet-name>
<servlet-class>com.attr.S2</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>Sum</servlet-name>
<url-pattern>/s1</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>Ssum1</servlet-name>
<url-pattern>/s2</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>amit1</servlet-name>
<servlet-class>com.servelets.Servlet1</servlet-class>
</servlet>
  
<servlet-mapping>
<servlet-name>amit1</servlet-name>
<url-pattern>/servlet1</url-pattern>
</servlet-mapping> 

<servlet>
<servlet-name>amit2</servlet-name>
<servlet-class>com.servelets.Servlet2</servlet-class>
</servlet> 

<servlet-mapping>
<servlet-name>amit2</servlet-name>
<url-pattern>/servlet2</url-pattern>
</servlet-mapping> 
  
</web-app>